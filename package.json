{
  "name": "gazatu-api-graphql-pgsql",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "tsc-watch": "tsc --watch",
    "db:init": "node ./scripts/exec \"./pgsql/bin/pg_ctl\" -D \"%INIT_CWD%/pgsql/data\" initdb -l en_US",
    "db:createuser": "node ./scripts/exec \"./pgsql/bin/createuser\" -s postgres",
    "db:createdb": "node ./scripts/exec \"./pgsql/bin/createdb\" -T template0 -l en_US -E UTF8 -O postgres gazatu_api",
    "db:start": "node ./scripts/exec \"./pgsql/bin/pg_ctl\" -D \"%INIT_CWD%/pgsql/data\" start",
    "db:stop": "node ./scripts/exec \"./pgsql/bin/pg_ctl\" -D \"%INIT_CWD%/pgsql/data\" stop",
    "start": "node dist/main",
    "dev": "node ./node_modules/nodemon/bin/nodemon.js",
    "test": "jest --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "argon2": "^0.26.0",
    "config": "^3.2.5",
    "dataloader": "^2.0.0",
    "graphql": "^14.6.0",
    "graphql-query-complexity": "^0.4.1",
    "graphql-relay": "^0.6.0",
    "graphql-subscriptions": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.11.0",
    "koa-bodyparser": "^4.2.1",
    "koa-json-error": "^3.1.2",
    "koa-logger": "^3.2.1",
    "pg": "^7.18.1",
    "reflect-metadata": "^0.1.13",
    "subscriptions-transport-ws": "^0.9.16",
    "type-graphql": "^0.17.6",
    "typedi": "^0.8.0",
    "typeorm": "^0.3.0-rc.17",
    "uuid": "^3.4.0",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "@types/config": "0.0.36",
    "@types/graphql": "^14.5.0",
    "@types/graphql-relay": "^0.4.11",
    "@types/jest": "^25.1.2",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/koa": "^2.11.1",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json-error": "^3.1.2",
    "@types/koa-logger": "^3.1.1",
    "@types/koa__cors": "^2.2.3",
    "@types/uuid": "^3.4.7",
    "@types/ws": "^7.2.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "typescript": "^3.7.5"
  }
}
