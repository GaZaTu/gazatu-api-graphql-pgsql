{
  "name": "gazatu-api-graphql-pgsql",
  "version": "0.4.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "tsc-watch": "tsc --watch",
    "db:init": "node ./scripts/exec \"./pgsql/bin/pg_ctl\" -D \"%INIT_CWD%/pgsql/data\" initdb -l en_US",
    "db:createuser": "node ./scripts/exec \"./pgsql/bin/createuser\" -s postgres",
    "db:createdb": "node ./scripts/exec \"./pgsql/bin/createdb\" -T template0 -l en_US -E UTF8 -O postgres gazatu_api",
    "db:start": "node ./scripts/exec \"./pgsql/bin/pg_ctl\" -D \"%INIT_CWD%/pgsql/data\" start",
    "db:stop": "node ./scripts/exec \"./pgsql/bin/pg_ctl\" -D \"%INIT_CWD%/pgsql/data\" stop",
    "migration:create": "typeorm migration:create",
    "migration:run": "typeorm migration:run",
    "migration:revert": "typeorm migration:revert",
    "start": "node dist/main",
    "dev": "node ./node_modules/nodemon/bin/nodemon.js",
    "test": "jest --watch"
  },
  "author": "gazatu",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.0.0",
    "argon2": "^0.27.1",
    "class-validator": "^0.13.1",
    "config": "^3.3.3",
    "dataloader": "^2.0.0",
    "graphql": "^15.5.0",
    "graphql-query-complexity": "^0.7.2",
    "graphql-relay": "^0.6.0",
    "graphql-subscriptions": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.1",
    "koa-bodyparser": "^4.3.0",
    "koa-json-error": "^3.1.2",
    "koa-logger": "^3.2.1",
    "node-fetch": "^2.6.6",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.28.1",
    "subscriptions-transport-ws": "^0.9.18",
    "type-graphql": "^1.1.1",
    "typedi": "^0.8.0",
    "typeorm": "^0.3.0-rc.21",
    "typescript": "^4.1.5",
    "uuid": "^8.3.2",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "@types/config": "0.0.38",
    "@types/graphql": "^14.5.0",
    "@types/graphql-relay": "^0.6.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/koa": "^2.13.0",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-json-error": "^3.1.2",
    "@types/koa-logger": "^3.1.1",
    "@types/koa__cors": "^3.0.2",
    "@types/koa__router": "^8.0.4",
    "@types/sharp": "^0.28.0",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7"
  }
}
